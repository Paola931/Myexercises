public class es24 {
    public static void main (String[]args) {
        char[] letters = {'a', 'b', 'c', 'a','d','e','f','a'};
        int count = 0;

        for (char c : letters) {
            if (c == 'a') {
                count++;
            }
        }
        if (count > 0) {
            System.out.println(count);
        } else {
            System.out.println(0);
        }

    }
}
