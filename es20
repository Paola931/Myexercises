
public class es20 {
    public static void main (String []args){
       int partenza = 6;
       int max = 3;
       risultato(partenza,max);
    }
    public static void risultato(int partenza,int max){
        for(int i = partenza;i >= max;i--){
            System.out.println(i);

        }
    }
}
